  <ItemGroup>
    <ProjectReference Include="../ShiftsLoggerLibrary/ShiftsLoggerLibrary.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.3" />
